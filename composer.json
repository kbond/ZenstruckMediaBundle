{
    "name": "zenstruck/media-bundle",
    "description": "Provides a simple media/file management GUI for Symfony2",
    "keywords": ["media", "file", "manager", "administration"],
    "homepage": "http://zenstruck.com/project/ZenstruckMediaBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.2|~3.0|~4.0|~5.0|~6.0"
    },
    "require-dev": {
        "cocur/slugify": "~0.8"
    },
    "suggest": {
        "cocur/slugify": "CocurSlugifyBundle is required for SlugifyFilenameFilter",
        "zenstruck/slugify-bundle": "If not using CocurSlugifyBundle, then this is required for the SlugifyFilenameFilter"
    },
    "autoload": {
        "psr-0": { "Zenstruck\\MediaBundle": "" }
    },
    "target-dir": "Zenstruck/MediaBundle"
}
